<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".instruments.instrumentHarmonica">
    <RelativeLayout
        android:id="@+id/armonica"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="775dp"
            android:layout_height="283dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="0dp">

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/armonicainstrumentomusic" />

            <Button
                android:id="@+id/cArmonica1"
                android:layout_width="55dp"
                android:layout_height="67dp"
                android:layout_marginBottom="104dp"
                android:background="@color/black"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/eArmonica2"
                app:layout_constraintHorizontal_bias="0.898"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="SpeakableTextPresentCheck" />

            <Button
                android:id="@+id/eArmonica2"
                android:layout_width="55dp"
                android:layout_height="67dp"
                android:layout_marginEnd="8dp"
                android:background="@color/black"
                app:layout_constraintBaseline_toBaselineOf="@+id/cArmonica1"
                app:layout_constraintEnd_toStartOf="@+id/gArmonica3" />

            <Button
                android:id="@+id/gArmonica3"
                android:layout_width="55dp"
                android:layout_height="67dp"
                android:layout_marginEnd="4dp"
                android:background="@color/black"
                app:layout_constraintBaseline_toBaselineOf="@+id/eArmonica2"
                app:layout_constraintEnd_toStartOf="@+id/dArmonica4" />

            <Button
                android:id="@+id/dArmonica4"
                android:layout_width="55dp"
                android:layout_height="67dp"
                android:layout_marginEnd="8dp"
                android:background="@color/black"
                app:layout_constraintBaseline_toBaselineOf="@+id/gArmonica3"
                app:layout_constraintEnd_toStartOf="@+id/fArmonica5" />

            <Button
                android:id="@+id/fArmonica5"
                android:layout_width="55dp"
                android:layout_height="67dp"
                android:layout_marginEnd="12dp"
                android:background="@color/black"
                app:layout_constraintBaseline_toBaselineOf="@+id/dArmonica4"
                app:layout_constraintEnd_toStartOf="@+id/g2Armonica6" />

            <Button
                android:id="@+id/g2Armonica6"
                android:layout_width="55dp"
                android:layout_height="67dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="104dp"
                android:background="@color/black"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/bArmonica7" />

            <Button
                android:id="@+id/bArmonica7"
                android:layout_width="55dp"
                android:layout_height="67dp"
                android:layout_marginEnd="8dp"
                android:background="@color/black"
                app:layout_constraintBaseline_toBaselineOf="@+id/g2Armonica6"
                app:layout_constraintEnd_toStartOf="@+id/e2Armonica8" />

            <Button
                android:id="@+id/e2Armonica8"
                android:layout_width="55dp"
                android:layout_height="67dp"
                android:layout_marginStart="528dp"
                android:layout_marginEnd="79dp"
                android:background="@color/black"
                app:layout_constraintBaseline_toBaselineOf="@+id/bArmonica7"
                app:layout_constraintEnd_toStartOf="@+id/aArmonica10"
                app:layout_constraintHorizontal_bias="0.674"
                app:layout_constraintStart_toStartOf="parent" />

            <Button
                android:id="@+id/f2Armonica9"
                android:layout_width="55dp"
                android:layout_height="67dp"
                android:background="@color/black"
                app:layout_constraintBaseline_toBaselineOf="@+id/e2Armonica8"
                app:layout_constraintEnd_toStartOf="@+id/aArmonica10"
                app:layout_constraintHorizontal_bias="0.583"
                app:layout_constraintStart_toEndOf="@+id/e2Armonica8" />

            <Button
                android:id="@+id/aArmonica10"
                android:layout_width="55dp"
                android:layout_height="67dp"
                android:layout_marginEnd="58dp"
                android:background="@color/black"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/e2Armonica8"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.518" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <LinearLayout
            android:id="@+id/top_bar"
            android:layout_width="match_parent"
            android:layout_height="132dp"
            android:background="@color/black"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/bree_serif"
                    android:text="ArmÃ³nica"
                    android:textColor="@android:color/white"
                    android:textSize="32sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>


    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>